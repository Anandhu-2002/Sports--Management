<section>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous" />

    <div class="container">

        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col"><span class="text">Date</span></th>
                    <th scope="col"><span class="text">Venu </span></th>
                    <th scope="col"><span class="text">Location </span></th>
                    <th scope="col"><span class="text">Match </span></th>
                    <th scope="col"><span class="text">Adults </span></th><span class="text">
                    <th scope="col"><span class="text">Child </span></th>
                    <th scope="col"><span class="text">Cancel </span></th>
                    <th scope="col"><span class="text">View </span></th>
                </tr>
            </thead>
            <tbody>                
                {{#each det}}
                <tr>
                    
                    
                    <td><span class="text">{{matchdet.date}} </span></td>
                    <td><span class="text">{{this.deliveryDetails.address}} </span></td>
                    <td><span class="text">{{this.deliveryDetails.pincode}} </span></td>
                    <td><span class="text">{{this.deliveryDetails.mobile}} </span></td>
                    <td><span class="text">Rs.{{this.total}} </span></td>
                    <td><span class="text">{{this.status}} </span></td>
                    <td>
                        <a href="/remove-order-products/{{this._id}}" class="btn btn-danger">Remove</a>
                        <a href="/view-order-products/{{this._id}}" class="btn btn-primary">View Products</a>
                        
                    </td>
                </tr>
                {{/each}}                
            </tbody>
        </table>

    </div>
</section>